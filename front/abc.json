{
  "name": "reakt",
  "options": {
    "type": "uxcore",
    "template": "https://github.com/nowa-webpack/template-uxcore/archive/v5.zip",
    "npm": "https://registry.npm.taobao.org",
    "vars": {
      "__LOCAL__": true
    },
    "buildvars": {
      "__LOCAL__": [false]
    },
    "pages": true,
    "injects": [
      "uxcore/assets/iconfont.css",
      "uxcore/assets/orange.css",
      "uxcore"
    ],
    "alias": {
      "components":"components",
      "db":"app/db.js",
      "variables":"app/variables.js"
    },
    "mockapi": {
      "/mock/user/query.json": "mock/user/query.js",
      "/mock/user/currentUser.json": "mock/user/currentUser.js"
    }
  }
}
